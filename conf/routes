# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


GET     /              controllers.FibonacciController.listSequences()
POST    /:parameterN           controllers.FibonacciController.retrieve(parameterN:Int)
POST    /              controllers.FibonacciController.create()
PUT     /              controllers.FibonacciController.update()
DELETE  /:parameterN           controllers.FibonacciController.delete(parameterN:Int)
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
